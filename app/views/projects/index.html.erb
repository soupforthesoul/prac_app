<h1>Listing projects</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Duration (hours)</th>
    <th>Startdate</th>
    <th>Enddate</th>
    <th>Priority</th>
    <th>Complete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= link_to project.name, project %></td>
    <td><%= project.description %></td>
    <td><%= project.duration %></td>
    <td><%= project.startdate %></td>
    <td><%= project.enddate %></td>
    <td><%= project.priority %></td>
    <td><%= project.complete %></td>
    <td><%= button_to 'Show', project, :method=>:get %></td>
    <td><%= button_to 'Edit', edit_project_path(project) %></td>
    <td><%= button_to "delete", project, :method=>:delete, :class=>:destroy %></td>
  </tr>
<% end %>
</table>

<br />


  <%= button_to 'New Project', new_project_path, :id => "new_project_button", :method=>:get %>
